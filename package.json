{
    "name": "backend",
    "version": "0.0.1",
    "description": "Awesome project developed with TypeORM.",
    "devDependencies": {
        "@types/express": "^4.17.6",
        "@types/node": "^8.0.29",
        "@typescript-eslint/eslint-plugin": "^3.2.0",
        "@typescript-eslint/parser": "^3.2.0",
        "nodemon": "^2.0.4",
        "sucrase": "^3.15.0",
        "ts-node": "3.3.0",
        "typescript": "3.3.3333"
    },
    "dependencies": {
        "body-parser": "^1.18.1",
        "express": "^4.15.4",
        "pg": "^7.3.0",
        "reflect-metadata": "^0.1.10",
        "typeorm": "0.2.25"
    },
    "scripts": {
        "start": "ts-node src/index.ts",
        "dev:server": "nodemon src/index.ts",
        "dev": "nodemon src/index.ts",
        "migration:generate": "npx ts-node ./node_modules/typeorm/cli.js migration:generate -n Initialize",
        "migration:run": "npx ts-node ./node_modules/typeorm/cli.js migration:run",
        "migration:revert": "npx  ts-node ./node_modules/typeorm/cli.js migration:revert"
    }
}
